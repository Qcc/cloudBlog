//- 首页左侧导航
div(class="nav-box")
  div.nav-style
    div(class="nav-list")
      a(href="/" class="logo")
        img(src="/news/img/logo.png", alt="网站logo" style="width: 108px; height: 27px;")
      ul(class="menu-list")
        each item in nav
          li
            a(href=item.path class="nav-item " + item.activeMenu)
              span=item.label